<h1>Edit Task</h1>
<%= form_with model: @task do |form| %>
<p>
  <%= form.label :title %><br>
  <%= form.text_field :title %>
</p>
<p>
  <%= form.label :details %><br>
  <%= form.text_area :details %>
</p>
<p>
  <%= form.label :completed %><br>
  <%= form.check_box :completed %>
</p>
<%= form.submit "Update Task "%>
<% end %>
